{
  "name": "jovie",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.17",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pre-push": "run-s typecheck test build",
    "check": "run-s validate build test",
    "check:ci": "run-s validate test:ci",
    "validate": "run-p format:check lint typecheck",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:ci": "NODE_OPTIONS=--max-old-space-size=4096 vitest run --reporter=verbose --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:synthetic": "playwright test --config=playwright.synthetic.config.ts",
    "test:e2e:golden-path": "playwright test tests/e2e/golden-path.spec.ts",
    "e2e:smoke": "playwright test -g @smoke",
    "e2e:smoke:noauth": "playwright test -g @smoke --config=playwright.config.noauth.ts",
    "e2e:full": "playwright test",
    "e2e:full:noauth": "playwright test --config=playwright.config.noauth.ts",
    "a11y:ci": "playwright test -g @a11y",
    "format": "biome format --write .",
    "format:check": "biome check .",
    "clean": "rm -rf .next out dist coverage .nyc_output",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "postbuild": "next-sitemap",
    "verify-artist-images": "node scripts/verify-artist-images.js",
    "update-artist-images": "node scripts/update-artist-images.js",
    "ensure-valid-artist-images": "node scripts/ensure-valid-artist-images.js",
    "prepare": "husky",
    "danger": "danger ci --fail-on-errors --verbose",
    "db:migrate": "node scripts/db-migrate.js",
    "db:seed": "tsx scripts/drizzle-seed.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "tsx scripts/drizzle-migrate.ts",
    "drizzle:migrate:preview": "GIT_BRANCH=preview tsx scripts/drizzle-migrate.ts",
    "drizzle:migrate:prod": "GIT_BRANCH=production tsx scripts/drizzle-migrate.ts",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:check": "drizzle-kit check",
    "drizzle:up": "drizzle-kit up",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/inter": "^5.2.6",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@stripe/stripe-js": "^7.8.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.0",
    "critters": "^0.0.25",
    "date-fns": "^3.2.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "flags": "^4.0.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.541.0",
    "next": "^15.5.0",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "posthog-js": "^1.260.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "server-only": "^0.0.1",
    "simple-icons": "^15.12.0",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@chromatic-com/storybook": "^4.1.1",
    "@clerk/clerk-js": "^5.0.0",
    "@clerk/dev-cli": "^0.0.12",
    "@clerk/testing": "^1.11.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@storybook/react": "^9.1.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.38.0",
    "@vercel/toolbar": "^0.1.32",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "danger": "^13.0.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-zod": "^0.8.3",
    "eslint": "^9",
    "eslint-config-next": "^15.5.0",
    "eslint-plugin-storybook": "^9.1.3",
    "husky": "^9.1.7",
    "ignore-loader": "^0.1.2",
    "jsdom": "^23.1.0",
    "lint-staged": "^16.1.5",
    "next-sitemap": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check",
      "biome format --write",
      "eslint --fix --max-warnings=0 --no-warn-ignored"
    ],
    "*.{json,md,css}": [
      "biome check",
      "biome format --write"
    ],
    "ignore": [
      "app/.well-known/**"
    ]
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
